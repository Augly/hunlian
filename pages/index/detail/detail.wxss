.banner_img {
  /* height: 750rpx; */
  position: relative;
  z-index: 100;
  background-color: RGBA(245, 245, 250, 1);
}

.banner_img image {
  width: 100%;
}

.name_box {
  width: 686rpx;
  border-radius: 10rpx;
  background: #fff;
  box-shadow: 0px 0px 5px rgba(0, 0, 0, 0.08);
  display: flex;
  align-items: center;
  justify-content: space-between;
  position: relative;
  margin: -50rpx auto 0 auto;
  /* left: 50%;
  margin-left: -343rpx;
  margin-top: -50rpx; */
  padding: 33rpx 32rpx;
  box-sizing: border-box;
}

.name {
  display: flex;
  font-size: 36rpx;
  align-items: center;
}

.sex_box {
  display: flex;
  align-items: center;
  border-radius: 3rpx;
  background-color: #ff99d3;
  padding: 0rpx 20rpx;
  box-sizing: border-box;
  color: #fff;
  font-size: 26rpx;
  margin-left: 18rpx;
  justify-content: center;
}
.sex_box .iconfont{
  font-size: 30rpx;
}

.hint {
  font-size: 24rpx;
  color: #999;
}

.item_list {
  background-color: #fff;
  margin-bottom: 16rpx;
  padding: 36rpx 30rpx;
}

.title {
  font-size: 32rpx;
  color: #c98c1c;
}

.real-name {
  font-size: 28rpx;
  color: #333;
}

.content_detail {
  height: 60rpx;
  line-height: 60rpx;
  background-color: #f7f7f7;
  font-size: 28rpx;
  color: #333;
  border-radius: 5rpx;
  padding: 0 36rpx;
  display: inline-block;
  margin-top: 16rpx;
}

.top {
  margin-top: 60rpx;
}

.certification {
  display: flex;
  justify-content: space-between;
}

.basic_information {
  display: flex;
  align-items: flex-end;
  margin-top: 35rpx;
}

.basic_information_item {
  margin-left: 68rpx;
}

.icon-diandiandianshu {
  font-size: 60rpx;
  color: #ccc;
  margin-left: auto;
}

.aaaa {
  display: flex;
  align-items: center;
}

.content_detail1 {
  height: 60rpx;
  line-height: 60rpx;
  background-color: #f7f7f7;
  font-size: 26rpx;
  color: #666;
  border-radius: 5rpx;
  padding: 0 36rpx;
  display: inline-block;
  margin-top: 16rpx;
  margin-left: 8rpx;
}

.icon-wode, .icon-3zeoutiaojian, .icon-xingqu {
  width: 48rpx;
  height: 48rpx;
  font-size: 32rpx;
  background-color: RGBA(174,156,128, 1);
  color: #fff;
  text-align: center;
  line-height: 48rpx;
  border-radius: 5rpx;
}

.bottom_btn {
  display: flex;
  justify-content: center;
  background-color: #fff;
  margin-bottom: 16rpx;
  padding: 10rpx 0;
}

.message_btn, .phone_btn {
  display: flex;
  height: 88rpx;
  background-color: rgba(174,156,128,1);
  border-radius: 44rpx;
  font-size: 30rpx;
  color: #fff;
  line-height: 88rpx;
  text-align: center;
  padding: 0 44rpx !important;
}

.icon-weidu, .icon-fuwuerji {
  font-size: 36rpx;
}

.words {
  margin-left: 22rpx;
}

.phone_btn {
  margin-left: 23rpx;
}

.MemberWrap {
  background-color: rgba(0, 0, 0, 0.8);
  position: fixed;
  top: 0;
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  z-index:110
}

.member {
  position: relative;
  min-width: 520rpx;
  box-sizing: border-box;
  padding: 32rpx 23rpx;
  background-color: rgba(255, 255, 255, 1);
  border-radius: 10rpx;
}

.mtitle {
  font-size: 32rpx;
  font-weight: bold;
  text-align: center;
  color: rgba(26, 26, 26, 1);
  margin-bottom: 37rpx;
}

.sumbit {
  width: 100%;
  height: 88rpx;
  background-color: #e9ad40;
  font-size: 32rpx;
  text-align: center;
  display: flex;
  align-items: center;
  justify-content: center;
  color: rgba(255, 255, 255, 1);
  margin-top: 32rpx;
}

.selectList {
  width: 100%;
  height: 100rpx;
  box-sizing: border-box;
  border-radius: 5rpx;
  border: 1rpx solid RGBA(235, 235, 235, 1);
  margin-top: 16rpx;
  padding: 24rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.sActive{
  background-color: RGBA(255, 248, 236, 1);
  border: 1rpx solid RGBA(233, 173, 64, 1);
}

.seLtitle {
  font-size: 28rpx;
  color: rgba(51, 51, 51, 1);
}

.seLmintitle {
  font-size: 24rpx;
  color: rgba(153, 153, 153, 1);
}
.selectR{
  display: flex;
  align-items:baseline;
}

.selectR_M{
  font-size: 24rpx;
  color: #E9AD40
}
.selectR_T{
  font-size: 36rpx;
  color: #E9AD40
}
.icon-quxiao1{
  position: absolute;
  right: 20rpx;
  top: 20rpx;
  color: RGBA(153, 153, 153, 1);
}

.mcontent{
  width: 100%;
  max-height: 500rpx;
  overflow-y: auto;
}
.mcontent view{
  font-size: 28rpx;
  text-align: center;
}