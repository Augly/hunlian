/* pages/index/version.wxss */
page{
  background-image: url('http://wxq.test.bronet.cn/images/denglubg.png')
}
.container {
  position: fixed;
  left: 0;
  height: 100%;
  width: 100%;
  top: 0;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
}

.banner_icon {
  width: 132rpx;
  height: 132rpx;
  border-radius: 20rpx;
  background-color: #000;
  font-size: 32rpx;
  font-style: italic;
  color: #fff;
  text-align: center;
  line-height: 132rpx;
  margin: 0 auto;
  margin-top: 269rpx;
}

.banner_title {
  font-size: 32rpx;
  font-style: italic;
  text-align: center;
  margin-top: 46rpx;
}

.banner_content {
  font-size: 32rpx;
  text-align: center;
}
.start_btn{
  position: fixed;
  width: 536rpx;
  height: 100rpx;
  font-size: 32rpx;
  color: #fff;
  line-height: 100rpx;
  text-align: center;
  background-color: RGBA(174, 156, 128, 1);
  border-radius: 50rpx;
  bottom: 176rpx;
  left: 0;
  right: 0;
  margin: 0 auto;
}
.note{
  width: 100%;
  height: 80rpx;
  font-size: 28rpx;
  color: RGBA(174, 156, 128, 1);
  text-align:center;
  position: fixed;
  bottom: 276rpx;
  line-height: 80rpx;
}

.banner{
  width: 65%;
  position:fixed;
  top: 100rpx;
}